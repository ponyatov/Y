%{
#include "sym.hpp"
%}

%defines

%union {
	string *c;
	biSymbol *s;
}

%token <s> S MODULE FFILE FEOF
%token <c> EOL

%%
sym :
	| sym MODULE S		{ W((new biModule($2,$3)),false); }
	| sym FFILE S EOL	{ W((new biFile  ($2,$3)),false); }
	| sym FEOF			{ W($2,false); if (bi_file) delete bi_file; }
	| sym EOL			{ W($2); }
	| sym S				{ W($2); }
;
%%

