# FSP skeleton

MODULE	= fsp
TITLE	= 'Free Syntax Programming'
ABOUT	= 'tiny portable declarative programming compiler'

allfiles = [
	files = [
		bi		= 'fsp.bI'
		log		= 'fsp.blog'
		yacc	= 'ypp.ypp'
		lex		= 'lpp.lpp'
		hpp		= 'hpp.hpp'
		meta	= 'meta.hpp'
		cpp		= 'cpp.cpp'
		make	= 'Makefile'
	]
		bat		= 'bat.bat'
		readme	= 'README.md'
		gitignore = '.gitignore'
]

