yacc = next + file @ 'ypp.ypp'
yacc += '%{\n#include "hpp.hpp"\n%}\n'
yacc += '%defines %union { Sym*o; }\n'
yacc += '%token <o> SYM\n'
yacc += '%type <o> scalar ex\n'
yacc += '%%\n'
yacc += 'REPL : | REPL ex { W($2->eval()); } ;\n'
yacc += 'scalar : SYM ;\n'
yacc += 'ex : scalar ;\n'
yacc += '%%\n'
