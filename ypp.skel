%token <o> DOC DIR 					/* doc: "docstring" .directive		*/
%left PERC
%left EQ
%left COLON
%left STR
%left DOT
%left AT
%left DOC
%%
REPL	: | REPL ex			{ W($2); W("\n------");					/* parsed */
							W($2->eval()); W("\n=======\n"); } ;	/* evaled */
		| DIR
		| ex DOC			{ $$=$1; $$->par["doc"]=$2; }
