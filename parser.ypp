%{
#include "bI.hpp"
%}

%defines

%union {
	char c;
	string *s;
	biObject *o;
}

%token <c> C EOL
%token <s> END

%token <o> DIRECTIVE NUM STR
%type <o> ex

%%
sym :
	| sym DIRECTIVE				{ W($2); }
	| sym ex					{ W($2); }
	| sym END					{ W($2); exit(0); }
	| sym EOL					{ W($2); }
	| sym C						{ W($2); }
;

ex : NUM
   | STR
;
%%

